{
  "name": "skylark-slax-runtime",
  "description": "The skylark shells widget",
  "version": "0.9.0",
  "keywords": [
    "skylark",
    "ui",
    "shells"
  ],
  "homepage": "https://github.com/skylark-slax/skylark-slax-runtime/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "slib build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-slax/skylark-slax-runtime.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-slax/skylark-slax-runtime/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-appify-routers": "git+https://github.com/skylark-appify/skylark-appify-routers.gitr",
    "skylark-appify-shells": "git+https://github.com/skylark-appify/skylark-appify-shells.gitr",
    "skylark-appify-spa": "git+https://github.com/skylark-appify/skylark-appify-spa.gitr",
    "skylark-data-entities": "git+https://github.com/skylark-data/skylark-data-entities.gitr",
    "skylark-domx": "git+https://github.com/skylark-domx/skylark-domx.gitr",
    "skylark-domx-files": "git+https://github.com/skylark-domx/skylark-domx-files.gitr",
    "skylark-domx-images": "git+https://github.com/skylark-domx/skylark-domx-images.gitr",
    "skylark-domx-plugins-colors": "git+https://github.com/skylark-domx-plugins/skylark-domx-plugins-colors.gitr",
    "skylark-io-caches": "git+https://github.com/skylark-io/skylark-io-caches.gitr",
    "skylark-io-streams": "git+https://github.com/skylark-io/skylark-io-streams.gitr",
    "skylark-jquery": "git+https://github.com/skylark-integration/skylark-jquery.gitr",
    "skylark-langx": "git+https://github.com/skylark-langx/skylark-langx.gitr",
    "skylark-net-http": "git+https://github.com/skylark-net/skylark-net-http.gitr"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  }
}
